package day10;

@:forward
abstract Line(String) from String
{
    static public var list:Array<Line>
        = Data.DATA_RAW.split("\n");
    
    static public var sample:Array<Line>
        = Data.SAMPLE_RAW.split("\n");
    
    public function reduce():Line
    {
        var line = this;
        var length:Int;
        do
        {
            length = line.length;
            line = line.split("()").join("")
                .split("[]").join("")
                .split("{}").join("")
                .split("<>").join("");
        }
        while(line.length < length);
        return line;
    }
    
    static var endMatcher = ~/[\)\}\]>]/;
    inline public function isCorrupted()
    {
        return endMatcher.match(this);
    }
    
    inline public function isUnfinished()
    {
        return false == isCorrupted();
    }
}

